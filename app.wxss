
/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding:0 20rpx;
  box-sizing: border-box;
  background:#F2F2F2;
} 
button::after{
  border:0;
}
page{
  padding-bottom:100rpx;
  background:#F2F2F2;
}
.redText{
  color:#F65B4E;
}
.deleteLine{
  text-decoration: line-through;
}
.floats:after{
    content:"";
    display:block;
    clear:both;
}
/*选择身份  */
.choseGender{
  width:510rpx;
  height:650rpx;
  border-radius:8px;
  background:#fff;
  padding:0 45rpx;
  position:absolute;
  top:303rpx;
  z-index:11;
  left:50%;
  margin-left:-300rpx;
}
.choseGender .genderTitle{
  font-size:34rpx;
  color:000;
  height:140rpx;
  line-height:140rpx;
  text-align:center;
}
.clears:after{
  content:"";
  display:block;
  clear:both;
}
.choseGender .gender{
  border:1px solid #F1F1F1;
  border-left:0;
  border-right:0;
  height:195rpx;
  display:flex;
}
.choseGender .lines{
  height:70rpx;
  background:#F1F1F1;
  width:2rpx;
  margin-top:57rpx;
}
.choseGender .headers{
  width:64rpx;
  height:72rpx;
  margin:0 auto;
}
.choseGender .headers image{
  width:64rpx;
  height:72rpx;
}
.choseGender .boys,.choseGender .girls{
  width:250rpx;
}
.choseGender .genderType{
  display:flex;
  margin-top:43rpx;
}
.choseGender .genderType button{
  width:160rpx;
  height: 60rpx;
  border:1px solid #F65B4E;
  border-radius: 5px;
  font-size:23rpx;
  color:#F65B4E;
  background:#fff;
}
.choseGender .next{
  margin-top:50rpx;
}
.choseGender .next button{
  width:480rpx;
  height:80rpx;
  font-size:28rpx;
  color:#fff;
  background:#34A4E4;
}
.choseGender .names{
  font-size:24rpx;
  text-align:center;
  margin-top:15rpx;
}
.girls .names{
  color:#F65B4E;
}
.boys,.girls{
  padding-top:57rpx;
}
.boys .names{
  color:#34A4E4;  
}
/*导航条  */
.btnArea{
  display:flex;
  position:fixed;
  height:120rpx;
  top:100%;
  width:100%;
  margin-top:-100rpx;
  align-items: center;
  justify-content: space-around;
  background:#fff;
}
.btnArea navigator{
  width:25%;  
  height:100%;
}
.btnArea navigator view{
  text-align:center;
  width:100%;
  height:100%;
  justify-content: space-around;
}
.btnArea navigator image{
  /* background-image:url(../images/page-icon.png); */
  background-repeat:no-repeat;
  background-position: 125px 80px;
  background-size:100%;
  height:40rpx;
  width:43rpx;
  /* margin:24rpx 0 0 0; */
  margin:20rpx 0 0 0;
}
.btnArea{
  color:#000;
  border-top:1px solid #E1E1E1;
}
.btnArea image,{
  width:42rpx;
  height:40rpx;
  margin-bottom:0;
}
.navName{
  font-size:22rpx;
  margin-top:-5rpx;
}
.blueText{
  color:#34A4E4;
}
.active2{
  background:#F65B4E !important;
  color:#fff !important;
}
.indexName {
  color:#34A4E4;
}
.swiper-box .wx-swiper-dot-active{
  width:60rpx !important;
}
.clears:after{
  content:"";
  display:block;
  clear:both;
}

.drawer_attr_box {  
  width: 100%;  
  overflow: hidden;  
  position: fixed;  
  bottom: 0;  
  left: 0;  
  z-index: 1001;  
  background: #fff;
  height:1063rpx;
  background:#fff;
  /* //965rpx; */
}  
.drawer_attr_box .line{
  border-bottom:1px solid #f2f2f2;
  width:730rpx;
  margin-left:20rpx;
}
.drawer_content {  
  padding: 20rpx 40rpx;  
  height: 470rpx;  
  overflow-y: scroll;  
}  
.drawer_title{  
  padding:20rpx;  
  font:42rpx "microsoft yahei";  
  text-align: center;  
}  
.drawer_screen {  
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: #000;  
  opacity: 0.2;  
  overflow: hidden;  
}  
swiper{
    -webkit-backface-visibility: hidden;
  backface-visibility: hidden; 
  -webkit-transform-style: preserve-3d;
  transform-style: preserve-3d; 
}