}
.askTitle{
  margin:20rpx 0;
  background:#fff;
  height:120rpx;
  display:flex;
  padding:20rpx 20rpx; 
}
.askTitle .imgbox{
  width:120rpx;
  height:120rpx;
  background:#ddd;
  border-radius:3px;
}
.imgbox image{
  width:100%;
  height:100%;
}
.askName{
  padding-left:20rpx;
}
.names{
  color:#000;
  font-size:32rpx;
  line-height:44rpx;
  font-weight:bold;
}
.zdPrice{
  font-size:24rpx;
  display:flex; 
  margin-top:10rpx;
}
.askTxt{
  height:230rpx;
  padding:20rpx;
  background:#fff;
  margin-bottom:80rpx;
}
.askArea{
  width:100%;
  height:100%;
  font-size:26rpx;
}
.btn button{
  width:630rpx;
  height:80rpx;
  color:#fff;
  line-height:80rpx;
  border:0;
  box-shadow:normal;
  border-radius:5px;
  background:#34A4E4;
  font-size:30rpx;
}
