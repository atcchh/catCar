}
.sp >view{
  float:left;
  height:100%;
}
.sp .left .imgBox{
    width:44rpx;
    height:44rpx;
    margin-top:88rpx;
}
image{
  width:100%;
  height:100%;
}
.sp .middle{
  width:210rpx;
}
.sp .middle .imgBox{

  margin-top:55rpx;
  margin-left:60rpx; 

  width:110rpx;
  height:110rpx;
}
.sp>.right{
  width:460rpx;
}
.names{
  color:#161616;
  font-size:24rpx;
  margin-top:16rpx;
  line-height: 35rpx;
}
.guige{
  color:#676767;
  font-size:24rpx;
  margin-top:9rpx;
  line-height: 40rpx;
}
.count{
  color:#000000;
  font-size:20rpx;
  margin-top:40rpx;
}
.count .left{
  float:left;
}
.count .right{
  float:right;
}
.right>button{
  width:50rpx;
  height:45rpx;
  box-sizing:border-box;
  border:1px solid #343434;
  background: #fff;
  padding:0;
  text-align: center;
  line-height: 38rpx;
  color:#000000;
}
.right>input{
    box-sizing:border-box;
    width:65rpx;
    height:42rpx !important;
    min-height:44rpx !important;
    border:1px solid #343434;
    text-align: center;
    line-height:40rpx;

}
.fl{
  float:left;
}
.right>button:first-child{
  border-right:0;
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
}

.right>button:last-child{
  border-left:0;
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
}
.footer{
  height:120rpx;
  line-height:120rpx;
  background: #fff;
  padding-left:20rpx;
}

.footer >view{
  float:left;
  height:120rpx;

}
.footer .left .imgBox{
  width:42rpx;
  height:44rpx;
  background:#fff;
  margin-top:16rpx;
}
.footer .left image{
    border:2px solid #f2f2f2;
  border-radius: 50%;
}
.footer >.left{
  width:125rpx;
  font-size:30rpx;
  color:#000;
}
.footer .middle{
  width:345rpx;

}
.footer .right button{
  width:250rpx;
  background:#F65B4E;
  font-size:30rpx;
  line-height:120rpx;
  height:120rpx;
  border:0;
  color:#fff;
}
.middle view:nth-child(1){
  font-size:28rpx;
  color:#000;
  margin-top:8rpx;
}
.middle view:nth-child(2){
  font-size:24rpx;
   color:#000;
}
.middle>view{
  height:60rpx;
  text-align: right;
  padding-right:32rpx;
  line-height: 60rpx;
}
.footer{
  position:fixed;
  top:100%;
  left:0;
  margin-top:-120rpx;
}
.footer .left {
  width:42rpx;
}